extends ../../../web_includes/common/layout

block includes
  include ../../../web_includes/common/common_includes.pug

block header
  i.fa-fw.fa.fa-signal
  | QAE

block main
  .row.row-fluid
    article.col-xs-10.col-sm-8.col-md-6.col-lg-6(style='max-width:450px')
        #wid-QAE-CmdAndData.jarviswidget.jarviswidget-color-blueDark(data-widget-editbutton='false')
          header
            span.widget-icon
              i.fa.fa-table
            h2 Housekeeping
          div
            .jarviswidget-editbox
              input.form-control(type='text')
            .widget-body
              .table-responsive
                table.table.table-condensed
                  tbody
                    tr
                      th Cmd Count
                      td
                        span(data-sage={tlm: [{name:'/CFS/QAE/CmdCounter'}]}) ---
                    tr
                      th Error Count
                      td
                        span(data-sage={tlm: [{name:'/CFS/QAE/ErrCounter'}]}) ---
                    tr
                      th State
                      td
                        span(data-sage={tlm: [{name:'/CFS/QAE/State'}]}) ---
                    tr
                      th Estimator State
                      td
                        span(data-sage={tlm: [{name:'/CFS/QAE/EstimatorState'}]}) ---
              footer
                button.btn.btn-primary(data-sage={cmd: {name:'/CFS/QAE/NoOp'}}) No-Op
                button.btn.btn-primary(data-sage={cmd: {name:'/CFS/QAE/Reset'}}) Reset
      
    .row.row-fluid
      article.col-xs-10.col-sm-8.col-md-6.col-lg-6(style="max-width:400px")
        #wid-SA.jarviswidget.jarviswidget-color-blueDark(data-widget-editbutton='false')
          header(style="overflow:hidden;")
            span.widget-icon
              i.fa.fa-table
            h2 Vehicle Attitude
          div
            .jarviswidget-editbox
              input.form-control(type='text')
            .widget-body
              .table-responsive
                table.table.table-condensed
                  tbody
                    tr
                      th Timestamp:
                      td
                        span.format-microseconds(data-sage={'tlm': [{'name': '/CFS/QAE/VA_Timestamp'}]}) ---
                    tr
                      th Roll Speed:
                      td
                        span(data-sage={'tlm': [{'name': '/CFS/QAE/VA_RollSpeed'}]}) ---
                    tr
                      th Pitch Speed:
                      td
                        span(data-sage={'tlm': [{'name': '/CFS/QAE/VA_PitchSpeed'}]}) ---
                    tr
                      th Yaw Speed:
                      td
                        span(data-sage={'tlm': [{'name': '/CFS/QAE/VA_YawSpeed'}]}) ---
                    tr
                      th Quaternion 0:
                      td
                        span(data-sage={'tlm': [{'name': '/CFS/QAE/VA_Q_0'}]}) ---
                    tr
                      th Quaternion 1:
                      td
                        span(data-sage={'tlm': [{'name': '/CFS/QAE/VA_Q_1'}]}) ---
                    tr
                      th Quaternion 2:
                      td
                        span(data-sage={'tlm': [{'name': '/CFS/QAE/VA_Q_2'}]}) ---
                    tr
                      th Quaternion 3:
                      td
                        span(data-sage={'tlm': [{'name': '/CFS/QAE/VA_Q_3'}]}) ---

    .row.row-fluid
      article.col-xs-10.col-sm-8.col-md-6.col-lg-6(style="max-width:400px")
        #wid-SA.jarviswidget.jarviswidget-color-blueDark(data-widget-editbutton='false')
          header(style="overflow:hidden;")
            span.widget-icon
              i.fa.fa-table
            h2 Control State
          div
            .jarviswidget-editbox
              input.form-control(type='text')
            .widget-body
