            td
              span(data-cdr={tlm: [{name: '/CF/CF_HkPacket_t/Eng.how_many_suspended'}],indicator:'text'}) ---
          tr
            th Files Sent
            td
              span(data-cdr={tlm: [{name: '/CF/CF_HkPacket_t/Eng.total_files_sent'}],indicator:'text'}) ---
          tr
            th Files Received
            td
              span(data-cdr={tlm: [{name: '/CF/CF_HkPacket_t/Eng.total_files_received'}],indicator:'text'}) ---
          tr
            th Unsuccessful Senders
            td
              span(data-cdr={tlm: [{name: '/CF/CF_HkPacket_t/Eng.total_unsuccessful_senders'}],indicator:'text'}) ---
          tr
            th Unsuccessful Receivers
