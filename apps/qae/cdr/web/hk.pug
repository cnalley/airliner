.row.os-theme-dark
  article.col-xs-12.col-sm-12
    header
      span.widget-icon
        i.fa.fa-table
      h2 QAE - Housekeeping
    div.widget-body.table-responsive
      table.table.table-condensed
        tbody
          tr
            th Cmd Count
            td
              div(data-cdr={tlm:[{name:'/QAE/QAE_HkTlm_t/usCmdCnt'}],indicator:'text'}) ---
          tr
            th Error Count
            td
              div(data-cdr={tlm:[{name:'/QAE/QAE_HkTlm_t/usCmdErrCnt'}],indicator:'text'}) ---
          tr
            th State
            td
              span(data-cdr={tlm: [{name:'/QAE/QAE_HkTlm_t/State'}], indicator:'text'}) ---
          tr
            th Estimator State
            td
              span(data-cdr={tlm: [{name:'/QAE/QAE_HkTlm_t/EstimatorState'}], indicator:'text'}) ---
         
    footer.btn-group(role="group")      
      button.btn.cdr-outline-primary(data-cdr={cmd:{name:'/QAE/Noop'},indicator:'cmd'}) No-Op    
      button.btn.cdr-outline-primary(data-cdr={cmd:{name:'/QAE/Reset'},indicator:'cmd'}) Reset
