extends ../../../web_includes/common/layout

block includes
  include ../../../web_includes/common/common_includes.pug

block header
  i.fa-fw.fa.fa-signal
  | MS5611 Barometric Altimeter Driver

block main
  .row.row-fluid
     article.col-xs-10.col-sm-8.col-md-6.col-lg-6(style='max-width:450px')
        #wid-MS5611-Housekeeping.jarviswidget.jarviswidget-color-blueDark(data-widget-editbutton='false')
          header
            span.widget-icon
              i.fa.fa-table
            h2 Housekeeping
          div
            .jarviswidget-editbox
              input.form-control(type='text')
            .widget-body
              .table-responsive
                table.table.table-condensed
                  tbody
                    tr
                      th Cmd Count
                      td
                        span(data-sage={tlm: [{name:'/CFS/MS5611/CmdCounter'}]}) ---
                    tr
                      th Error Count
                      td
                        span(data-sage={tlm: [{name:'/CFS/MS5611/ErrCounter'}]}) ---
                    tr
                      th State
                      td
                        span(data-sage={tlm: [{name:'/CFS/MS5611/State'}]}) ---
              footer
                button.btn.btn-primary(data-sage={cmd:{name:'/CFS/MS5611/NoOp'}}) No-Op
                button.btn.btn-primary(data-sage={cmd:{name:'/CFS/MS5611/Reset'}}) Reset
                button.btn.btn-primary(data-sage={cmd:{name:'/CFS/MS5611/Diag'}}) Diag

  .row.row-fluid
    article.col-xs-10.col-sm-6.col-md-6.col-lg-6(style="max-width:450px")
        #wid-MS5611-Config.jarviswidget.jarviswidget-color-blueDark(data-widget-editbutton='false')
          header(style="overflow:hidden;")
            span.widget-icon
              i.fa.fa-table
            h2 Configuration (Diag)
          div
            .jarviswidget-editbox
              input.form-control(type='text')
            .widget-body
                table.table.table-hover.table-condensed.table-compressed
                  tbody
                    tr
                      th Coef 0
                      td
                        span(data-sage={'tlm': [{'name': "/CFS/MS5611/Coef0"}]}) ---
                    tr
                      th Coef 1
                      td
                        span(data-sage={'tlm': [{'name': "/CFS/MS5611/Coef1"}]}) ---
                    tr
                      th Coef 2
                      td
                        span(data-sage={'tlm': [{'name': "/CFS/MS5611/Coef2"}]}) ---
                    tr
                      th Coef 3
                      td
                        span(data-sage={'tlm': [{'name': "/CFS/MS5611/Coef3"}]}) ---
                    tr
                      th Coef 4
                      td
                        span(data-sage={'tlm': [{'name': "/CFS/MS5611/Coef4"}]}) ---
                    tr
                      th Coef 5
                      td
                        span(data-sage={'tlm': [{'name': "/CFS/MS5611/Coef5"}]}) ---
                    tr
                      th Coef 6
                      td
                        span(data-sage={'tlm': [{'name': "/CFS/MS5611/Coef6"}]}) ---
                    tr
                      th Coef 7
                      td
                        span(data-sage={'tlm': [{'name': "/CFS/MS5611/Coef7"}]}) ---
                      
  .row.row-fluid
    article.col-xs-10.col-sm-8.col-md-6.col-lg-6(style='max-width:450px')
        #wid-MS5611-Measurements.jarviswidget.jarviswidget-color-blueDark(data-widget-editbutton='false')
          header(style="overflow:hidden;")
            span.widget-icon
              i.fa.fa-table
            h2 Measurements (Diag)
          div
            .jarviswidget-editbox
              input.form-control(type='text')
            .widget-body
                table.table.table-condensed
                  tbody
                    tr
                      th Raw Temperature
                      td
                        span(data-sage={'tlm': [{'name': "/CFS/MS5611/RawTemperature"}]}) ---
                    tr
                      th Raw Pressure
                      td
                        span(data-sage={'tlm': [{'name': "/CFS/MS5611/RawPressure"}]}) ---
                    tr
                    tr
                      th Temperature
                      td
                        span(data-sage={'tlm': [{'name': "/CFS/MS5611/Temperature"}]}) ---
                    tr
                      th Pressure
                      td
                        span(data-sage={'tlm': [{'name': "/CFS/MS5611/Pressure"}]}) ---
                    tr
                      th Altitude
                      td
                        span(data-sage={'tlm': [{'name': "/CFS/MS5611/Altitude"}]}) ---

  .row
    +app_specific_telemetry_control("MS5611")
                                              
  .row          
    +app_specific_mempools("MS5611")
    
  .row
    +app_specific_application_control("MS5611") 
    +registered_applications() 
  
  .row
    +app_specific_table_control("MS5611")
    
  .row
    +app_specific_scheduler_control("MS5611")
