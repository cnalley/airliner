  .row.row-fluid
    article.col-xs-10.col-sm-6.col-md-5.col-lg-4(style='max-width:350px')
      #wid-CFE-ES-PerfMon-Performance.jarviswidget.jarviswidget-color-blueDark(data-widget-editbutton='false')
        header
          span.widget-icon
            i.fa.fa-table
          h2 Performance
        div
          .jarviswidget-editbox
            input.form-control(type='text')
          .widget-body
            .table-responsive
              table.table.table-condensed
               tbody
                tr
                 th State
                 td 
                   span(data-sage={tlm:[{name:'/CFS/CFE_ES/PerfState'}]}) ---
                tr
                 th Mode
                 td
                   span(data-sage={tlm:[{name:'/CFS/CFE_ES/PerfMode'}]}) ---
                tr
                 th Trigger count
                 td
                   span(data-sage={tlm:[{name:'/CFS/CFE_ES/PerfTriggerCount'}]}) ---
                tr
                 th Start
                 td
                   span(data-sage={tlm:[{name:'/CFS/CFE_ES/PerfDataStart'}]}) ---
                tr
                 th End
                 td
                   span(data-sage={tlm:[{name:'/CFS/CFE_ES/PerfDataEnd'}]}) ---
                tr
                 th Count
                 td
                   span(data-sage={tlm:[{name:'/CFS/CFE_ES/PerfDataCount'}]}) ---
                tr
                 th Data to Write
                 td
                   span(data-sage={tlm:[{name:'/CFS/CFE_ES/PerfDataToWrite'}]}) ---
            footer
              .btn-group.dropup
                button.btn.btn-primary(data-sage={cmd: {name: '/CFS/CFE_ES/PerfStart'}}) Start
                button.btn.btn-primary(data-sage={cmd: {name: '/CFS/CFE_ES/PerfStop'}}) Stop/Write

