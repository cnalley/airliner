.row.os-theme-dark
  article.col-xs-12.col-sm-12
    header
      span.widget-icon
        i.fa.fa-table
      h2 CS - CFE Core
    div.widget-body.table-responsive
      table.table.table-condensed
        tbody
          tr
            th State
            td
              div(data-cdr={tlm:[{name:'/CS/CS_HkPacket_t/CfeCoreCSState'}],indicator:'text'}) ---
            td
              .btn-group(role="group")
                button.btn.cdr-outline-primary(data-cdr={cmd:{name:'/CS/EnableCfeCore'},indicator:'cmd'}) Enable 
                button.btn.cdr-outline-primary(data-cdr={cmd:{name:'/CS/DisableCfeCore'},indicator:'cmd'}) Disable
          tr
            th Error Count
            td
              span(data-cdr={tlm: [{name:'/CS/CS_HkPacket_t/CfeCoreCSErrCounter'}], indicator:'text'}) ---
          tr
            th Baseline
            td
              span(data-cdr={tlm: [{name:'/CS/CS_HkPacket_t/CfeCoreBaseline'}], indicator:'text'}) ---
            td
              .btn-group(role="group")
                button.btn.cdr-outline-primary(data-cdr={cmd:{name:'/CS/ReportBaselineCfeCore'},indicator:'cmd'}) Report 
                button.btn.cdr-outline-primary(data-cdr={cmd:{name:'/CS/RecomputeBaselineCfeCore'},indicator:'cmd'}) Recompute